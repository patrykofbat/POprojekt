<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane fx:id="root" prefHeight="600" prefWidth="800" stylesheets="@style.css" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controller.Controller">
    <HBox fx:id="hBoxUp" alignment="CENTER_LEFT" prefHeight="100" prefWidth="800" spacing="50">

        <Button fx:id="btnNext" alignment="BASELINE_RIGHT" onMouseClicked="#nextModel" styleClass="btn" text="START" textAlignment="CENTER" />
      <opaqueInsets>
         <Insets left="10.0" />
      </opaqueInsets>

    </HBox>

</AnchorPane>
